  "moduleNameMapper": {
    "@nst-typegoose/nst-typegoose/(.*)": "<rootDir>/../libs/nst-typegoose/src/$1",
    "@nst-typegoose/nst-typegoose": "<rootDir>/../libs/nst-typegoose/src"
  }
}